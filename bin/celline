#!/bin/bash
EXEC_DIR=$(dirname "$(
    cd "$(dirname "$0")" || exit
    pwd
)")
cd "${EXEC_DIR}" || exit
if [ "$1" = "static" ]; then
    poetry run python "$EXEC_DIR/celline/initializer/initialize.py" "${EXEC_DIR}" "$2" "$3"
else
    poetry run python -m celline "${EXEC_DIR}" "${PROJ_DIR}" "${1}" "${2}" "${3}" "${4}" "${5}"
fi
